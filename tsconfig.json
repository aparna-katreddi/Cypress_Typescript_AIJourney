{
  "compilerOptions": {
    "target": "esnext",
    "outDir": "./ignoreJS",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node"],
    "typeRoots": ["./node_modules/@types", "./cypress/support"]
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules"]
}
